{
  "name": "personal-assistance",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "node --env-file=.env.local --import tsx scripts/run-migrate.ts",
    "db:migrate:run": "npx tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d lib/db/typeorm-config.cli.ts",
    "db:migration:create": "npx tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "db:migration:generate": "npx tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d lib/db/typeorm-config.cli.ts",
    "db:schema:sync": "npx tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d lib/db/typeorm-config.cli.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.975.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "pg": "^8.17.2",
    "react": "19.2.3",
    "react-day-picker": "^9.4.4",
    "react-dom": "19.2.3",
    "reflect-metadata": "^0.2.2",
    "strnum": "2.1.2",
    "tailwind-merge": "^3.4.0",
    "typeorm": "^0.3.20",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
